<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="wut_search" version="3.0">
  <display-name>WUT Search</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>web</servlet-name>
    <servlet-class>com.wut.search.resource.WebResource</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>web</servlet-name>
    <url-pattern>/web</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>website</servlet-name>
    <servlet-class>com.wut.search.resource.WebsiteResource</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>website</servlet-name>
    <url-pattern>/website</url-pattern>
  </servlet-mapping>
</web-app>
