<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="wut_code" version="3.0">
  <display-name>WUT Code</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>template</servlet-name>
    <servlet-class>com.wut.resources.templates.TemplateResource</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>template</servlet-name>
    <url-pattern>/template</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>javascript</servlet-name>
    <servlet-class>com.wut.resources.javascript.JavaScriptResource</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>javascript</servlet-name>
    <url-pattern>/javascript</url-pattern>
  </servlet-mapping>
</web-app>
